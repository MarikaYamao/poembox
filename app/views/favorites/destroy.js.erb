<% if @type == 'LikePhoto' %>
    $("#like-form-<%= @photo.id %>").html("<%= escape_javascript(render('favorites/like_button', type: @type, photo: @photo)) %>");
<% elsif @type == 'LikePoem' %>
    $("#like-form-<%= @poem.id %>").html("<%= escape_javascript(render('favorites/like_button', type: @type, poem: @poem)) %>");
<% end %>