<% if @type == 'LikePhoto' %>
    $("#like-form-photo-<%= @photo.id %>").html("<%= escape_javascript(render('favorites/unlike_button', type: @type, photo: @photo)) %>");
<% elsif @type == 'LikePoem' %>
    $("#like-form-poem-<%= @poem.id %>").html("<%= escape_javascript(render('favorites/unlike_button', type: @type, poem: @poem)) %>");
<% end %>